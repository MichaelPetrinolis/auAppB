{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Chrome",
      "type": "chrome",
      "request": "launch",
      "url": "http://localhost:8080",
      "webRoot": "${workspaceRoot}/src",
      "userDataDir": "${workspaceRoot}/.chrome",
      "sourceMapPathOverrides": {
        "webpack:///./src/*": "${webRoot}/*",
        "webpack:///./node_modules/auModA/*": "${workspaceRoot}/submodules/auModA/*",
        "webpack:///./node_modules/auModC/*": "${workspaceRoot}/submodules/auModC/*"
      }
    }
  ]
}
